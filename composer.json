{
    "name": "confl1kt/news-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataNewsBundle",
    "keywords": ["blog", "sonata", "news", "post"],
    "homepage": "https://sonata-project.org/bundles/news",
    "license": "MIT",
    "require": {
        "symfony/symfony": "~2.7",
        "sonata-project/admin-bundle": "~2.3",
        "sonata-project/media-bundle": "~2.3",
        "sonata-project/classification-bundle": "~2.2",
        "sonata-project/formatter-bundle": "~2.3",
        "sonata-project/easy-extends-bundle": "~2.1",
        "sonata-project/intl-bundle": "~2.1",
        "sonata-project/core-bundle": "^2.3.2",
        "jms/serializer-bundle": "~0.11",
        "egeloen/ckeditor-bundle": "~2.0"
    },
    "require-dev": {
        "doctrine/orm": "*",
        "sonata-project/doctrine-orm-admin-bundle": "~2.2",
        "sonata-project/notification-bundle": "~2.2",
        "fabpot/php-cs-fixer": "~0.1|~1.0"
    },
    "conflict": {
        "jms/serializer": "<0.13"
    },
    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "~2.2",
        "sonata-project/notification-bundle": "~2.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Sonata\\NewsBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4.x-dev"
        }
    }
}
